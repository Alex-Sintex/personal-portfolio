RewriteEngine On

# Allow specific pages
RewriteCond %{REQUEST_URI} !^/gastos_diarios\.html$
RewriteCond %{REQUEST_URI} !^/balance\.html$

# Allow static files (CSS, JS, images, fonts, etc.)
RewriteCond %{REQUEST_URI} !\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot)$

# Redirect everything else
RewriteRule ^.*$ /gastos_diarios.html [L]